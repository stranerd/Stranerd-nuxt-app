{
	"name": "stranerd",
	"version": "1.0.0",
	"private": true,
	"author": "Stranerd Dev",
	"description": "The Stranerd app written in Nuxt",
	"scripts": {
		"dev": "yarn env:copy:app && nuxt",
		"start": "nuxt start",
		"generate": "nuxt generate",
		"build": "yarn generate",
		"lint": "eslint src/ --ext .ts,.js,.vue --fix && stylelint src/**/*.scss src/**/*.vue --fix",
		"resources:android": "cordova-res-generator -p android,ios && node resources/sync.js android",
		"resources:ios": "cordova-res-generator -p android,ios && node resources/sync.js ios",
		"cap:android": "yarn generate && npx cap sync android && npx cap open android",
		"cap:ios": "yarn generate && npx cap sync ios && npx cap open ios",
		"env:copy:example": "copy env.example.json env.json",
		"env:copy:app": "node bin/copy-env-app",
		"prepare": "husky install"
	},
	"dependencies": {
		"@capacitor/android": "3.3.0",
		"@capacitor/core": "^3.3.0",
		"@capacitor/ios": "^3.3.0",
		"@capacitor/network": "^1.0.6",
		"@capacitor/toast": "^1.0.6",
		"@ionic/core": "^5.8.5",
		"@ionic/pwa-elements": "^3.0.2",
		"@ionic/vue": "^5.8.5",
		"@nuxtjs/composition-api": "^0.29.3",
		"@stranerd/validate": "^1.6.0",
		"@stripe/stripe-js": "^1.21.1",
		"@vue-stripe/vue-stripe": "^4.2.8",
		"axios": "^0.24.0",
		"bootstrap": "^5.1.3",
		"cookie": "^0.4.1",
		"core-js": "^3.19.1",
		"firebase": "^9.2.0",
		"flutterwave-vue-v3": "^1.0.5",
		"ionicons": "^5.5.4",
		"nuxt": "^2.15.8",
		"socket.io-client": "^4.3.2",
		"stripe": "^8.186.1",
		"sweetalert2": "^11.1.9",
		"vue-chat-scroll": "^1.4.0",
		"vue-google-login": "^2.0.5",
		"vue2-editor": "^2.10.3"
	},
	"devDependencies": {
		"@capacitor/cli": "^3.3.0",
		"@commitlint/cli": "^14.1.0",
		"@commitlint/config-conventional": "^14.1.0",
		"@nuxt/types": "^2.15.8",
		"@nuxt/typescript-build": "^2.1.0",
		"@nuxtjs/eslint-config-typescript": "^7.0.2",
		"@nuxtjs/eslint-module": "^3.0.2",
		"@nuxtjs/pwa": "^3.3.5",
		"@nuxtjs/style-resources": "^1.2.1",
		"@nuxtjs/tailwindcss": "^4.2.1",
		"@types/cookie": "^0.4.1",
		"@types/vue2-editor": "^2.6.1",
		"@typescript-eslint/parser": "^5.3.0",
		"babel-eslint": "^10.1.0",
		"cordova-res-generator": "^0.4.2",
		"eslint": "^8.1.0",
		"eslint-plugin-nuxt": "^3.0.0",
		"eslint-plugin-vue": "^8.0.3",
		"husky": "^7.0.4",
		"node-sass": "^5.0.0",
		"nuxt-compress": "^5.0.0",
		"nuxt-purgecss": "^1.0.0",
		"sass-loader": "^10.2.0",
		"stylelint": "^14.0.1",
		"stylelint-config-standard": "^23.0.0",
		"stylelint-scss": "^4.0.0"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"license": "ISC"
}
